{
  "name": "zensical",
  "compatibility_date": "2025-06-01",
  "assets": {
    "directory": "./site",
    "html_handling": "auto-trailing-slash",
    "not_found_handling": "404-page",
    "run_worker_first": false
  },
  "build": {
    "command": "pip install -r requirements.txt && zensical build --clean"
  },
  "workers_dev": true,
  "preview_urls": true
}
